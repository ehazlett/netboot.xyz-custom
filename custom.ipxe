#!ipxe
###
### netboot.xyz-custom menu
###

:custom
clear custom_choice
menu Infra
item --gap Installers
item debian ${space} Debian Installer
choose custom_choice || goto custom_exit
echo ${cls}
goto ${custom_choice}
goto custom_exit

:debian
kernel http://boothttp.int/debian-installer/amd64/linux
initrd http://boothttp.int/debian-installer/amd64/initrd.gz
boot || goto custom_exit

:custom_exit
chain utils.ipxe
exit
